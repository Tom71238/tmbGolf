<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:context="com.tbrey1gmail.tmbgolf.View.gc_selected"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical">

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:ems="10"
        android:inputType="date"
        android:onClick="setDateOnClick"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="46dp"
        tools:text="mm/dd/yy" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="Date of Round"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="58dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Course Played"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="33dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Name of Course"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="187dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:ems="10"
        android:hint="select"
        android:inputType="none"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tees Played"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="111dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Course Rating"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="182dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        android:ems="10"
        android:hint="based on course selected"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="227dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:text="Course Slope"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="234dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:ems="10"
        android:hint="based on course selected"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="169dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:text="Select Player 2 (optional)"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="375dp" />

    <TextView
        android:id="@+id/textViewA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:text="Select Player 3 (optional)"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textViewB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:text="Select Player 4 (optional)"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="458dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Keep Score"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="153dp" />

    <EditText
        android:id="@+id/editTextC"
        android:layout_width="376dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text="Hole: 01 02 03 04 05 06 07 08 09 10 11 12 13 14 15 16 17 18"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="253dp" />

    <EditText
        android:id="@+id/editTextD"
        android:layout_width="376dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="4dp"
        android:ems="10"
        android:text=" PAR: 03 03 03 03 04 04 04 04 05 05 05 05 04 04 04 04 04 04"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="290dp" />

    <EditText
        android:id="@+id/editTextE"
        android:layout_width="376dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:text=" HDC: 01 02 03 04 05 06 07 08 09 10 11 12 13 14 15 16 17 18"
        android:textSize="12sp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="327dp" />

</android.support.constraint.ConstraintLayout>
